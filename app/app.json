{
  "expo": {
    "name": "5WH Media",
    "slug": "5wh-media",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#DC143C",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.5whmedia.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.fivewhmedia.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "backgroundColor": "#DC143C"
      },
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.VIBRATE",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.WAKE_LOCK",
        "android.permission.ACCESS_NOTIFICATION_POLICY",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-notifications",
      [
        "expo-av",
        {
          "microphonePermission": "Allow 5WH Media to access your microphone for audio playback."
        }
      ]
    ],
    "scheme": "5wh-media",
    "extra": {
      "eas": {
        "projectId": "ac6a614d-96b1-4e78-9877-fd51b385c69f"
      }
    }
  }
}
